#include <iostream>
using namespace std;

int main() {
    srand(time(nullptr));

    const int ARRAY_SIZE = 30;
    int array[ARRAY_SIZE];

    cout << "Please fill an array with integer elements:" << endl;

    for (int i = 0; i < ARRAY_SIZE; i++)
    {
        // cout << "Enter a value for element array[" << i << "] : ";
        // cin >> array[i];
        array[i] = rand() % 56;
    }

    for (int i = 0; i < ARRAY_SIZE; i++)
    {
        if (array[i] = rand() % 56)
        {
             cout << array[i] << " ";
        }
    }

    cout << "Our array contains:" << endl;
    for (int i = 0; i < ARRAY_SIZE; i++)
    {
        cout <<  "Fibonacci numbers: 1 2 3 5 8 13 21 34 55" << " ";

        return 0;
    }


}
